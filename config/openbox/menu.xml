			</action>
		</item>
		<item label="FocusWriter">
			<action name="Execute">
				<command>focuswriter</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Vim">
			<action name="Execute">
				<command>konsole -e vim</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="GEdit">
			<action name="Execute">
				<command>gedit</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
